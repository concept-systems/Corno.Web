@model Corno.Web.Areas.Masters.ViewModels.MiscMaster.MiscMasterDto
@{
    Layout = "~/Views/Shared/_LayoutDetails.cshtml";
}
@{
    ViewBag.Index = "Index";
    ViewBag.Create = "Create";
    ViewBag.Edit = "Edit";
    ViewBag.Details = "Details";
    ViewBag.Delete = "Delete";
}
@{
    Page.Title = Model.MiscType;
    ViewBag.MainMenu = "Masters";
    ViewBag.CurrentSitemap = Model.MiscType;
    ViewBag.BoxTitle = "View MiscMaster :" + Model.MiscType;
    ViewBag.Area = "Masters";
    ViewBag.Controller = "MiscMaster";
    ViewBag.SubmitAction = ViewBag.Details;
}

@Html.HiddenFor(m => m.Id)
@Html.HiddenFor(m => m.MiscType)

<div class="box-content">
    <div class="box">

        <div class="box-content">
            <div class="tab-content">
                <div class="control-group">
                    <label class="control-label"> Name <span style="color: red"></span>:</label>
                    <div class="controls">
                        <label class="control-label">@Model.Name</label>
                    </div>
                </div>
                <div class="control-group">
                    <label class="control-label">Description :</label>
                    <div class="controls">
                        <label class="control-label">@Model.Description</label>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>



